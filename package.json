    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tsconfig": "*",
    "turbo": "latest"
  },
  "name": "final-project",
  "packageManager": "bun@1.0.0",
